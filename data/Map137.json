ariableId":1,"variableValid":false,"variableValue":0},"directionFix":false,"image":{"characterIndex":0,"characterName":"","direction":2,"pattern":0,"tileId":0},"list":[{"code":0,"indent":0,"parameters":[]}],"moveFrequency":3,"moveRoute":{"list":[{"code":0,"parameters":[]}],"repeat":true,"skippable":false,"wait":false},"moveSpeed":3,"moveType":0,"priorityType":0,"stepAnime":false,"through":false,"trigger":0,"walkAnime":true}],"x":13,"y":12},{"id":3,"name":"EV003","note":"","pages":[{"conditions":{"actorId":1,"actorValid":false,"itemId":1,"itemValid":false,"selfSwitchCh":"A","selfSwitchValid":false,"switch1Id":1,"switch1Valid":false,"switch2Id":1,"switch2Valid":false,"variableId":1,"variableValid":false,"variableValue":0},"directionFix":false,"image":{"characterIndex":0,"characterName":"","direction":2,"pattern":0,"tileId":0},"list":[{"code":121,"indent":0,"parameters":[410,410,0]},{"code":121,"indent":0,"parameters":[202,202,0]},{"code":230,"indent":0,"parameters":[200]},{"code":101,"indent":0,"parameters":["",0,0,0]},{"code":401,"indent":0,"parameters":["\\c[7]\\N[3]\\c[0]"]},{"code":401,"indent":0,"parameters":["\"Они верят, что в пророчествах говорится о тебе\"."]},{"code":230,"indent":0,"parameters":[80]},{"code":101,"indent":0,"parameters":["",0,0,0]},{"code":401,"indent":0,"parameters":["\\c[7]\\N[6]\\c[0]"]},{"code":401,"indent":0,"parameters":["\"Хм. Эти древние тексты - лишь детский лепет...\""]},{"code":101,"indent":0,"parameters":["",0,0,0]},{"code":401,"indent":0,"parameters":["\\c[7]\\N[6]\\c[0]"]},{"code":401,"indent":0,"parameters":["\"Разумеется, это не значит, что мне не стоит использовать "]},{"code":401,"indent":0,"parameters":["их в своих интересах\"."]},{"code":230,"indent":0,"parameters":[80]},{"code":101,"indent":0,"parameters":["",0,0,0]},{"code":401,"indent":0,"parameters":["\\c[7]\\N[3]\\c[0]"]},{"code":401,"indent":0,"parameters":["\"...\""]},{"code":101,"indent":0,"parameters":["",0,0,0]},{"code":401,"indent":0,"parameters":["\\c[7]\\N[3]\\c[0]"]},{"code":401,"indent":0,"parameters":["\"А ведь и правда - многие детали совпали с пугающей "]},{"code":401,"indent":0,"parameters":["точностью\"."]},{"code":101,"indent":0,"parameters":["",0,0,0]},{"code":401,"indent":0,"parameters":["\\c[7]\\N[3]\\c[0]"]},{"code":401,"indent":0,"parameters":["\"В пророчествах говорится о светловолосом мужчине, о том, "]},{"code":401,"indent":0,"parameters":["кто сравнится с богами как по силе, так и по красоте...\""]},{"code":101,"indent":0,"parameters":["",0,0,0]},{"code":401,"indent":0,"parameters":["\\c[7]\\N[6]\\c[0]"]},{"code":401,"indent":0,"parameters":["\"Ха! Я-то думал, все старые боги уродливы донельзя\"."]},{"code":101,"indent":0,"parameters":["",0,0,0]},{"code":401,"indent":0,"parameters":["\\c[7]\\N[6]\\c[0]"]},{"code":401,"indent":0,"parameters":["\"Только не говори, что тоже на это купилась?\""]},{"code":101,"indent":0,"parameters":["",0,0,0]},{"code":401,"indent":0,"parameters":["\\c[7]\\N[3]\\c[0]"]},{"code":401,"indent":0,"parameters":["\"Говорят, Нильван из новых богов весьма хороша собой. "]},{"code":401,"indent":0,"parameters":["Прямо как был когда-то Измученный...\""]},{"code":230,"indent":0,"parameters":[80]},{"code":101,"indent":0,"parameters":["",0,0,0]},{"code":401,"indent":0,"parameters":["\\c[7]\\N[3]\\c[0]"]},{"code":401,"indent":0,"parameters":["\"В тексте также говорится о том, кто объединит враждующие "]},{"code":401,"indent":0,"parameters":["государства силой не золотого посоха, а дюжины вил\"."]},{"code":101,"indent":0,"parameters":["",0,0,0]},{"code":401,"indent":0,"parameters":["\\c[7]\\N[6]\\c[0]"]},{"code":401,"indent":0,"parameters":["\"Что ж, сейчас меня ненавидят все короли поголовно, "]},{"code":401,"indent":0,"parameters":["поэтому здесь не поспорить...\""]},{"code":230,"indent":0,"parameters":[80]},{"code":101,"indent":0,"parameters":["",0,0,0]},{"code":401,"indent":0,"parameters":["\\c[7]\\N[6]\\c[0]"]},{"code":401,"indent":0,"parameters":["\"Я сделаю все, чтобы объединить эти земли, сказано так в "]},{"code":401,"indent":0,"parameters":["пророчествах или нет\"."]},{"code":101,"indent":0,"parameters":["",0,0,0]},{"code":401,"indent":0,"parameters":["\\c[7]\\N[6]\\c[0]"]},{"code":401,"indent":0,"parameters":["\"Чего бы мне это ни стоило\"."]},{"code":230,"indent":0,"parameters":[80]},{"code":101,"indent":0,"parameters":["",0,0,0]},{"code":401,"indent":0,"parameters":["\\c[7]\\N[6]\\c[0]"]},{"code":401,"indent":0,"parameters":["\"Однако...\""]},{"code":101,"indent":0,"parameters":["",0,0,0]},{"code":401,"indent":0,"parameters":["\\c[7]\\N[6]\\c[0]"]},{"code":401,"indent":0,"parameters":["\"Я чувствую, что очень близок к достижению своих целей, но "]},{"code":401,"indent":0,"parameters":["последний шаг мне неясен... Будто я блуждаю в тумане\"."]},{"code":101,"indent":0,"parameters":["",0,0,0]},{"code":401,"indent":0,"parameters":["\\c[7]\\N[6]\\c[0]"]},{"code":401,"indent":0,"parameters":["\"Я не вполне уверен, что следую верному пути\"."]},{"code":230,"indent":0,"parameters":[80]},{"code":101,"indent":0,"parameters":["",0,0,0]},{"code":401,"indent":0,"parameters":["\\c[7]\\N[3]\\c[0]"]},{"code":401,"indent":0,"parameters":["\"В смысле? Ты в шаге от основания собственного королевства "]},{"code":401,"indent":0,"parameters":["и становления одним из самых значимых правителей нашего "]},{"code":401,"indent":0,"parameters":["времени!\""]},{"code":101,"indent":0,"parameters":["",0,0,0]},{"code":401,"indent":0,"parameters":["\\c[7]\\N[3]\\c[0]"]},{"code":401,"indent":0,"parameters":["\"Ты станешь великим королем! Я уверена! Народ уже тебя "]},{"code":401,"indent":0,"parameters":["любит!\""]},{"code":101,"indent":0,"parameters":["",0,0,0]},{"code":401,"indent":0,"parameters":["\\c[7]\\N[6]\\c[0]"]},{"code":401,"indent":0,"parameters":["\"...\""]},{"code":230,"indent":0,"parameters":[120]},{"code":250,"indent":0,"parameters":[{"name":"alll-mer_bell_87020__zgump__bell-0105_01","volume":60,"pitch":100,"pan":0}]},{"code":224,"indent":0,"parameters":[[25